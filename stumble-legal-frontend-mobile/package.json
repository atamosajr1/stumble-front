{
  "name": "stumblelegal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "init": "npm install && npx pod-install && git submodule update --init --recursive && git submodule update --remote && npx husky init",
    "codegen:rtkq": "ts-node _scripts/rtk-query-codegen.ts rtkq.config.ts",
    "run-aws-secrets": "node ./_scripts/aws-secrets.mjs",
    "get-aws-secrets:dev": "env-cmd -f ./.deploy/.env.dev npm run run-aws-secrets",
    "get-aws-secrets:stage": "env-cmd -f ./.deploy/.env.stage npm run run-aws-secrets",
    "get-aws-secrets:prod": "env-cmd -f ./.deploy/.env.prod npm run run-aws-secrets",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios",
    "ios:stage": "react-native run-ios",
    "ios:prod": "react-native run-ios",
    "ios:run-build": "git tag ios-build -f -m \"[ios build]\" && git push --tag --force",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:dev": "react-native run-android --mode=devDebug",
    "android:stage": "react-native run-android --variant=stageDebug",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:apk": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "android:build-dev": "cd android && ./gradlew assembleDev",
    "android:build-stage": "cd android && ./gradlew assembleStage",
    "android:build-prod": "cd android && ./gradlew assembleProd",
    "android:bundle": "cd android && ./gradlew bundleProd",
    "android:run-build": "git tag android-build -f -m \"[android build]\" && git push --tag --force",
    "run-builds": "git tag android-build -f -m \"[android build]\" && git tag ios-build -f -m \"[ios build]\" && git push --tag --force",
    "test": "jest",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint --quiet src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky"
  },
